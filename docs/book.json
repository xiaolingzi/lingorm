{
  "title": "lingorm - orm framework for go",
  "plugins": [
    "lb2br",
    "plantuml-cloud",
    "-sharing",
    "hide-published-with",
    "footnote-string-to-number",
    "anchors",
    "image-captions",
    "ad",
    "chapter-fold",
    "search-plus",
    "google-adsense",
    "github",
    "github-buttons",
    "tbfed-pagefooter"
  ],
  "pluginsConfig": {
    "plantuml-cloud": {
      "protocol": "http",
      "host": "plantuml",
      "port": 1608,
      "blockRegex": "^```p?uml((.*[\r\n])+?)?```$"
    },
    "image-captions": {
      "caption": "Fig. _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
    },
    "google-adsense": {
      "ads": [
        {
          "slot": "9902936962",
          "client": "ca-pub-6914140396303354",
          "location": ".page-inner section",
          "format": "auto"
        }
      ]
    },
    "github": {
      "url": "https://github.com/xiaolingzi/lingorm"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "xiaolingzi",
          "repo": "lingorm",
          "type": "star",
          "size": "small"
        },
        {
          "user": "xiaolingzi",
          "type": "follow",
          "size": "small",
          "width": "150",
          "count": false
        }
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &#169; xiaolingzi",
      "modify_label": "Last modifiedï¼š",
